{
  "manifest_version": 3,
  "name": "Phishing Detector",
  "version": "1.0",
  "description": "Extracts email content and sends it to n8n for analysis.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://mail.google.com/*","http://localhost:5678/*","https://pink-hound-93.hooks.n8n.cloud/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Analyze Phishing Risk"
  }
}